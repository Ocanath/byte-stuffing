            "miDebuggerPath": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build-test",
            "sourceFileMap": {
                "/build/glibc-SzIz7B": "/usr/src/glibc"
            }
        }
    ]
} 