{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug test_PPP",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/test/out/test_PPP/test_PPP.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
